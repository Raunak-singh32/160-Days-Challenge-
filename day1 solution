class Solution {
    public int getSecondLargest(int[] arr) {
        // code here
        Arrays.sort(arr);
        int n = arr.length-1;
        int largest = arr[n];
        for(int i=n-1;i>=0;i--){
            if(arr[i]<largest){
                return arr[i];
            }
        }
        return -1;
    }
}
